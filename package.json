    "license": "GPL-3.0-or-later",
    "bugs": {
        "url": "https://github.com/Tenniscenter-Buchs/tennisbuchs/issues"
    },
    "homepage": "https://github.com/Tenniscenter-Buchs/tennisbuchs#readme",
    "dependencies": {
        "body-parser": "^1.19.0",
        "concurrently": "^5.3.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "nodemon": "^2.0.5",
        "pg": "^8.4.1",
        "swagger-jsdoc": "^4.3.0",
        "swagger-ui-express": "^4.1.4"
    },
    "devDependencies": {
        "eslint": "^7.11.0",
        "eslint-config-prettier": "^6.12.0",
